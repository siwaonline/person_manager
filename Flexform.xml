<T3DataStructure>
	<sheets>
		<Einstellungen>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexsitename>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.sitename</label>
							<config>
								<type>input</type>
							</config>
						</TCEforms>
					</settings.flexsitename>
					<settings.flexsignature>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.signature</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexsignature>
				</el>
			</ROOT>
		</Einstellungen>
		<Texte>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.msgs</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexcheckmail>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.ancheckmail</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Thank you&lt;/h1&gt;&lt;h3&gt;Please check your email-account and confirm your registration.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexcheckmail>
					<settings.flexconfirm>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.anconfirm</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Thank you&lt;/h1&gt;&lt;h3&gt;You have been successfully added to our system.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexconfirm>
					<settings.flexerr>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.anerror</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Error&lt;/h1&gt;&lt;h3&gt;This person doen't exist.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
