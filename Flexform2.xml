<T3DataStructure>
	<sheets>
		<Einstellungen>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexsitename>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.sitename</label>
							<config>
								<type>input</type>
							</config>
						</TCEforms>
					</settings.flexsitename>
					<settings.flexsignature>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.signature</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexsignature>
				</el>
			</ROOT>
		</Einstellungen>
		<Texte>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.msgs</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexcheckmailleave>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.abcheckmail</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Thank you&lt;/h1&gt;&lt;h3&gt;Please check your email-account and confirm your deregistration.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexcheckmailleave>
					<settings.flexisunsubscribed>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.abalready</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Error&lt;/h1&gt;&lt;h3&gt;This email-address is already unsubscribed.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexisunsubscribed>
					<settings.flexleave>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.abnomail</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Error&lt;/h1&gt;&lt;h3&gt;This email-address is not registered in our system.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexleave>
					<settings.flexunsubscribe>
						<TCEforms>
							<label>LLL:EXT:person_manager/Resources/Private/Language/locallang_db.xlf:flex.abconfirm</label>
							<config>
								<type>text</type>
								<cols>24</cols>
								<rows>10</rows>
								<eval>trim</eval>
								<default>&lt;h1&gt;Thank you&lt;/h1&gt;&lt;h3&gt;You have been successfully removed from our system.&lt;/h3&gt;</default>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.flexunsubscribe>
				</el>
			</ROOT>
		</Texte>
	</sheets>
</T3DataStructure>